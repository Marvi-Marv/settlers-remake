	<startjob name="gohome" />

	<!-- going home job -->
	<job name="gohome" 		type="GO_TO" 		successjob="hide" 		failjob="hide" 		dx="0" dy="3" />
	<job name="hide" 		type="HIDE" 		successjob="start" 		failjob="start" 		/>

	<job name="start" 		type="WAIT" 		successjob="checkflour" failjob="hide" 		time="3.0" />
	<job name="checkflour" 	type="NOT_FULL" 	successjob="checkcrop" 	failjob="hide" 		dx="-1" dy="5" />
	<job name="checkcrop" 	type="AVAILABLE" 	successjob="show" 		failjob="hide" 		dx="5" 	dy="3" material="CROP" />
	<job name="show" 		type="SHOW" 		successjob="go_to_crop" failjob="hide" 		dx="0" dy="3" />

	<job name="go_to_crop" 	type="GO_TO" 		successjob="take" 		failjob="gohome" 	dx="5" dy="3" />
	<job name="take" 		type="TAKE" 		successjob="go_back" 	failjob="gohome" 	material="CROP" />
	<job name="go_back" 	type="GO_TO" 		successjob="startwork" 	failjob="gohome" 	dx="0" dy="3" />
	<job name="startwork" 	type="START_WORKING" successjob="hide1" 	failjob="makeflour"		/>
	<job name="hide1" 		type="HIDE" 		successjob="makeflour" 	failjob="start" 		/>
	<job name="makeflour" 	type="WAIT" 		successjob="changeMat2" failjob="start" 	time="5" />
	<job name="changeMat2" 	type="SET_MATERIAL" successjob="show2" 		failjob="hide" 		material="FLOUR" />
	<job name="show2" 		type="SHOW" 		successjob="walkflour1" failjob="hide" 		dx="0" dy="3" />

	<job name="walkflour1"	type="WALK" 		successjob="stopwork" 	failjob="gohome" 	direction="SOUTH_WEST" />
	<job name="stopwork" 	type="STOP_WORKING" successjob="walkflour2" failjob="changeMat2"	/>
	<job name="walkflour2"	type="WALK" 		successjob="go_drop" 	failjob="gohome" 	direction="SOUTH_WEST" />
	<job name="go_drop"		type="GO_TO" 		successjob="look" 		failjob="gohome" 	dx="-1" dy="5"/>

	<job name="look" 		type="LOOK_AT" 		successjob="drop" 		failjob="hide" 		direction="NORTH_EAST" />
	<job name="drop" 		type="DROP" 		successjob="check1"		failjob="hide" 		material="FLOUR" />

	<job name="check1"  	type="NOT_FULL" 	successjob="check2" 	failjob="gohome" 	dx="-1" dy="5" />
	<job name="check2" 		type="AVAILABLE"	successjob="go_to_crop" failjob="gohome" 	dx="5" 	dy="3" material="CROP" />

